{
  "name": "wizardry-next",
  "author": "Alexey Pautov-Brevnov <alexey.pautovbrevnov@gmail.com>",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/ && stylelint **/*.scss",
    "lintCode": "eslint src/",
    "lintStyles": "stylelint **/*.scss",
    "lint:fix": "eslint src/ --fix && stylelint **/*.scss --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "commit": "cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.5.3",
    "@prisma/client": "5.20.0",
    "bcryptjs": "2.4.3",
    "clsx": "2.0.0",
    "lodash": "4.17.21",
    "next": "14.2.13",
    "next-auth": "5.0.0-beta.22",
    "prisma": "5.20.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "use-breakpoint": "4.0.1",
    "zod": "3.22.4",
    "zustand": "5.0.0-rc.2"
  },
  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@commitlint/cz-commitlint": "18.2.0",
    "@types/bcrypt": "5.0.2",
    "@types/bcryptjs": "2.4.6",
    "@types/lodash": "4.14.200",
    "@types/node": "20.8.10",
    "@types/react": "18.2.36",
    "autoprefixer": "10.4.16",
    "commitizen": "4.3.0",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-wizardry": "0.1.1",
    "eslint-plugin-wizardry-fsd": "0.2.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.10",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "sass": "1.69.5",
    "stylelint": "15.11.0",
    "stylelint-config-clean-order": "5.2.0",
    "stylelint-config-standard-scss": "11.1.0",
    "stylelint-no-unsupported-browser-features": "7.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.6.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
